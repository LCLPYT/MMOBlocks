  "authors": [
    "DerBerliner",
    "LCLP"
  ],
  "contact": {
    "homepage": "https://github.com/LCLPYT/MMOQuark",
    "sources": "https://github.com/LCLPYT/MMOQuark"
  },

  "license": "CC BY-NC-SA 3.0 DE",
  "icon": "assets/quark/icon.png",

  "environment": "*",
  "entrypoints": {
